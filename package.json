{
  "name": "vcms",
  "version": "1.8.1",
  "description": "a tiny node cms (express, objection, express-session/redis, and more)",
  "author": "Valentin Degenne <vdegenne@gmail.com>",
  "repository": "vdegenne/node-vcms",
  "license": "BSD-3-Clause",
  "main": "lib/vcms",
  "keywords": [
    "vcms",
    "cms",
    "starter"
  ],
  "scripts": {
    "run:dev": "cd test && NODE_ENV=dev node app",
    "clean": "rimraf lib",
    "build": "yarn clean && tsc",
    "install:database": "psql -U postgres -a -f ./test/sql/install.sql",
    "uninstall:database": "psql -U postgres -a -f ./test/sql/uninstall.sql",
    "test": "LOGGERS=0 mocha",
    "test:watch": "yarn clean && tsc-then -- LOGGERS=0 mocha",
    "format": "find src test -iname '*.ts' | xargs clang-format --style=file -i"
  },
  "dependencies": {
    "@types/chai": "^4.1.2",
    "@types/chai-as-promised": "^7.1.0",
    "@types/command-line-args": "^4.0.2",
    "@types/connect-redis": "0.0.7",
    "@types/express": "^4.11.1",
    "@types/express-session": "^1.15.8",
    "@types/js-yaml": "^3.11.1",
    "@types/knex": "^0.14.12",
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.6.1",
    "@types/readline-sync": "^1.4.3",
    "@types/redis": "^2.8.6",
    "@types/supertest": "^2.0.4",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "clang-format": "^1.0.41-c",
    "command-line-args": "^5.0.2",
    "compression": "^1.7.2",
    "connect-redis": "^3.3.3",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "knex": "^0.14.6",
    "mocha": "^5.0.5",
    "morgan": "^1.9.0",
    "objection": "^1.1.7",
    "pg": "^7.4.1",
    "readline-sync": "^1.4.9",
    "redis": "^2.8.0",
    "req-control": "0.0.1",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.4",
    "spdy": "^3.4.7",
    "supertest": "^3.0.0",
    "tsc-then": "^1.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1"
  },
  "typings": "./lib/vcms.d.ts",
  "devDependencies": {
    "@types/compression": "^0.0.36",
    "@types/spdy": "^3.4.4"
  }
}
