{
  "name": "vcms",
  "version": "1.9.5",
  "description": "a tiny node cms (express, objection, express-session/redis, and more)",
  "author": "Valentin Degenne <vdegenne@gmail.com>",
  "repository": "vdegenne/node-vcms",
  "license": "BSD-3-Clause",
  "main": "lib/vcms",
  "keywords": [
    "vcms",
    "cms",
    "starter"
  ],
  "scripts": {
    "run:dev": "cd test && NODE_ENV=dev node app",
    "clean": "rimraf lib",
    "build": "yarn clean && tsc",
    "install:database": "psql -U postgres -a -f ./test/sql/install.sql",
    "uninstall:database": "psql -U postgres -a -f ./test/sql/uninstall.sql",
    "test": "LOGGERS=0 mocha",
    "test:watch": "yarn clean && tsc-then -- LOGGERS=0 mocha",
    "format": "find src test -iname '*.ts' | xargs clang-format --style=file -i"
  },
  "dependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/connect-redis": "^0.0.7",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.8",
    "@types/js-yaml": "^3.11.2",
    "@types/knex": "^0.15.1",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.9",
    "@types/readline-sync": "^1.4.3",
    "@types/redis": "^2.8.6",
    "@types/supertest": "^2.0.4",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "clang-format": "^1.2.4",
    "command-line-args": "^6.0.0-preview.1",
    "compression": "^1.7.3",
    "connect-redis": "^3.4.0",
    "express": "^5.0.0-alpha.7",
    "express-session": "^1.15.6",
    "knex": "^2.3.0",
    "mocha": "^5.2.0",
    "morgan": "^1.9.1",
    "objection": "^3.0.1",
    "pg": "^8.5.1",
    "readline-sync": "^1.4.9",
    "redis": "^2.8.0",
    "req-control": "^0.0.1",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.9",
    "spdy": "^4.0.0",
    "supertest": "^3.3.0",
    "tsc-then": "^1.1.0",
    "tslint": "^5.11.0",
    "typescript": "^3.2.0-rc"
  },
  "typings": "./lib/vcms.d.ts",
  "devDependencies": {
    "@types/compression": "^0.0.36",
    "@types/spdy": "^3.4.4"
  }
}
